{
	"name": "npm lookup",
	"version": "2.0.0",
	"description": "Turn selected text on a webpage into an npm package lookup",
	"homepage_url": "https://github.com/radiovisual/github-npm-lookup",
	"manifest_version": 2,
	"minimum_chrome_version": "38",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"contextMenus"
	],
	"background": {
		"scripts": ["menu.js"]
	}
}
